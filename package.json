{
  "name": "phoenix-atlas",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "deploy": "ng build --prod --output-path ./docs --base-href \"./\" && cp ./docs/index.html ./docs/404.html",
    "deploy:single": "ng build --configuration singleEvent --prod --output-path ./docs --base-href \"./\" && cp ./docs/index.html ./docs/404.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.5",
    "@angular/common": "~11.2.5",
    "@angular/compiler": "~11.2.5",
    "@angular/core": "~11.2.5",
    "@angular/forms": "~11.2.5",
    "@angular/platform-browser": "~11.2.5",
    "@angular/platform-browser-dynamic": "~11.2.5",
    "@angular/router": "~11.2.5",
    "phoenix-event-display": "^1.4.0",
    "phoenix-ui-components": "^1.4.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular/cli": "~11.2.4",
    "@angular/compiler-cli": "~11.2.5",
    "@types/node": "^12.11.1",
    "typescript": "~4.1.5"
  }
}
